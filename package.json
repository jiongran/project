{
  "name": "project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "alpha": "vue-cli-service build --mode alpha",
    "beta": "vue-cli-service build --mode beta",
    "tep": "node scripts/template",
    "comtep": "node scripts/componentsTemplate",
    "lint": "vue-cli-service lint",
    "commitizen": "commitizen init cz-conventional-changelog --save --save-exact",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -w -r 0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.6.2",
    "element-ui": "^2.12.0",
    "js-cookie": "^2.2.1",
    "moment": "^2.24.0",
    "nprogress": "^0.2.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-i18n": "^8.14.1",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.2",
    "vuex-module-decorators": "^0.10.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/js-cookie": "^2.2.2",
    "@types/nprogress": "^0.2.0",
    "@types/webpack-env": "^1.14.0",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-typescript": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.25",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^2.3.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.1.5",
    "style-resources-loader": "^1.2.1",
    "ts-node-dev": "^1.0.0-pre.43",
    "typescript": "^3.4.3",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-cli-plugin-style-resources-loader": "^0.1.3",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.40.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard",
      "@vue/typescript"
    ],
    "rules": {
      "indent": [
        "off",
        2
      ],
      "space-before-function-paren": [
        "off",
        2
      ],
      "vue/array-bracket-spacing": "error",
      "vue/arrow-spacing": "error",
      "vue/block-spacing": "error",
      "vue/brace-style": "error",
      "vue/camelcase": "error",
      "vue/comma-dangle": "error",
      "vue/component-name-in-template-casing": "error",
      "vue/eqeqeq": "error",
      "vue/key-spacing": "error",
      "vue/match-component-file-name": "error",
      "vue/object-curly-spacing": "error"
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "sourceType": "module"
    },
    "plugins": [
      "vue"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "keywords": [
    "vue",
    "typescript",
    "template",
    "element-ui"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
